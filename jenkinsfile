pipeline { 
     agent any
     stages{
        stage('Checkout GIT') 
        { 
        steps{ 
            echo 'Pulling...'   
              git branch: 'mourad', credentialsId: 'mourad', url: 'https://github.com/MouradBENRAHAL/Devops.git'
          }
        }
        stage ('Nexus') {
            steps {
                sh 'mvn deploy -DskipTests'
      }
    } 
    

        }
        }
